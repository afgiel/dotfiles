[alias]
    s = status
    ci = commit -v
    co = checkout
    a = add
    l = log
    update = "!f(){ git fetch && git rebase origin/main; };f"
    fp = push --force-with-lease
    sync = "!f() { git diff-index --quiet HEAD -- && git fetch origin && git reset --hard @{u} || echo 'You have uncommitted changes. Commit or stash them first.'; }; f"
[core]
    editor = vim
